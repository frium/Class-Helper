{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
			"^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
			"^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/score/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/examination/index",
			"style": {
				"navigationBarTitleText": "考试日程"
			}
		},
		{
			"path": "pages/personalCenter/index",
			"style": {
				"navigationBarTitleText": "个人主页"
			}
		}
	],
	"tabBar": {
		"color": "#7A7E83", // 默认文字颜色
		"selectedColor": "#007AFF", // 选中文字颜色
		"borderStyle": "white", // 上边框颜色
		"backgroundColor": "#f8f8f8", // 背景色
		"position": "bottom", // 可选 bottom/top
		"list": [
			{
				"pagePath": "pages/index/index",
				"text": "课表"
			},
			{
				"pagePath": "pages/score/index",
				"text": "成绩"
			},
			{
				"pagePath": "pages/examination/index",
				"text": "考试"
			},
			{
				"pagePath": "pages/personalCenter/index",
				"text": "我的"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"titleView": false
		}
	}
}